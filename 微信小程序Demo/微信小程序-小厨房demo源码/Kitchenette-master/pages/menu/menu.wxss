.page-header {
	width: 100%;
	height: 52px;
	line-height: 52px;
	background-color: #fff;
	text-align: center;
}
.page-header-title {
	text-align: center;
}
.page-body {
	background-color: #f3f2f0;
}
.item_list {
	height: 60px;
	background-color: #fff;
    border-bottom: 1px solid #f3f2f0;
    padding: 12px 0 10px 14px;
    overflow: hidden;
}
.menu_img {
	display: inline-block;
	width: 80px;
	height: 60px;
	float: left;
}
.img {
	width: 100%;
	height: 100%;
}
.item_text {
	display: inline-block;
	float: left;
	margin-left: 10px;
	width: 66%;
	height: 60px;
	position: relative;
}
.menu_name {
	display: block;
	color: #333;
	font-size: 16px;
}
.menu_des {
	position: absolute;
	left: 0;
	bottom: 0;
	display: block;
	float: left;
	font-size: 14px;
	color: #999;
}